{
  "name": "vnindex-analysis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "copy-data": "node -e \"const fs=require('fs');const path=require('path');const srcDir='src/data';const publicDir='public/data';if(!fs.existsSync(publicDir))fs.mkdirSync(publicDir,{recursive:true});if(fs.existsSync(srcDir)){fs.readdirSync(srcDir).forEach(f=>{if(f.endsWith('.json')){fs.copyFileSync(path.join(srcDir,f),path.join(publicDir,f));console.log('📁 Copied:',f);}});}\"",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@embed-tools/components": "workspace:*",
    "lightweight-charts": "^5.0.8",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.0.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "tailwindcss": "^4.1.10",
    "vite": "^6.3.5"
  }
}